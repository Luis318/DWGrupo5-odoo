            <component
              refId="Package\FactInventariosOdoo\Origen de ADO NET"
              componentClassID="Microsoft.ManagedComponentHost"
              contactInfo="Consume datos de SQL Server, OLE DB, ODBC u Oracle mediante el correspondiente proveedor de datos de .NET Framework. Se usa una instrucción Transact-SQL para definir el conjunto de resultados. Por ejemplo, se extraen datos de SQL Server con el proveedor de datos de .NET Framework para SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; Todos los derechos reservados; http://www.microsoft.com/sql/support;4"
              description="Consume datos de SQL Server, OLE DB, ODBC u Oracle mediante el correspondiente proveedor de datos de .NET Framework. Se usa una instrucción Transact-SQL para definir el conjunto de resultados. Por ejemplo, se extraen datos de SQL Server con el proveedor de datos de .NET Framework para SQL Server."
              name="Origen de ADO NET"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Especifica la instrucción SQL utilizada por el componente para extraer datos."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH uom AS (
  SELECT id, COALESCE(name-&gt;&gt;'es_MX',name-&gt;&gt;'es_ES',name-&gt;&gt;'es_CO',name-&gt;&gt;'en_US',name-&gt;&gt;'0')::text AS uom_name
  FROM public.uom_uom
),
pt_names AS (
  SELECT id, COALESCE(name-&gt;&gt;'es_MX',name-&gt;&gt;'es_ES',name-&gt;&gt;'es_CO',name-&gt;&gt;'en_US',name-&gt;&gt;'0')::text AS producto_nombre
  FROM public.product_template
),
picking_type AS (
  SELECT id,
         COALESCE(name-&gt;&gt;'es_MX',name-&gt;&gt;'es_ES',name-&gt;&gt;'es_CO',name-&gt;&gt;'en_US',name-&gt;&gt;'0')::text AS picking_type_name,
         code::text AS picking_type_code
  FROM public.stock_picking_type
),
-- Snapshot de stock por producto/ubicación (para la columna Stock)
quant AS (
  SELECT product_id, location_id,
         COALESCE(SUM(quantity),0)::numeric(18,6) AS qty_on_hand,
         COALESCE(SUM(reserved_quantity),0)::numeric(18,6) AS qty_reserved
  FROM public.stock_quant
  GROUP BY product_id, location_id
),
/* ================= movimientos base (recepción/entrega/transferencia) */
base_mov AS (
  SELECT
    sml.id::int                                    AS id_evento,
    sml."date"::timestamp                          AS fecha,
    COALESCE(sml.product_id,sm.product_id)::int    AS id_producto,
    COALESCE(pp.default_code,'SIN_CODIGO')::text   AS codigo_producto,
    ptn.producto_nombre::text                      AS nombre_producto,
    pt.categ_id::int                               AS id_categoria,
    COALESCE(pc.name,'Sin categoría')::text        AS nombre_categoria,
    sml.location_id::int                           AS id_ubic_origen,
    sml.location_dest_id::int                      AS id_ubic_destino,
    COALESCE(sml.quantity,sml.quantity_product_uom,sm.quantity,0)::numeric(18,6) AS cantidad,
    u.uom_name::text                               AS unidad_medida,
    ptp.picking_type_code::text                    AS picking_type_codigo,
    (CASE ptp.picking_type_code
       WHEN 'incoming' THEN 'recepcion'
       WHEN 'outgoing' THEN 'entrega'
       WHEN 'internal' THEN 'transferencia'
       ELSE COALESCE(ptp.picking_type_code,'otro')
     END)::text                                    AS tipo_evento,
    sm.state::text                                 AS estado_move,
    sp.id::int                                     AS id_picking,
    COALESCE(sp.name,'')::text                     AS codigo_picking,
    pol.order_id::int                              AS id_orden_compra,
    COALESCE(po.name,'')::text                     AS codigo_orden_compra,
    sm.production_id::int                          AS id_orden_produccion,
    COALESCE(mp.name,'')::text                     AS codigo_orden_produccion,
    sml.lot_id::int                                AS id_lote,
    COALESCE(sml.lot_name,'')::text                AS lote_serie,
    COALESCE(sm.reference,'')::text                AS referencia,
    sm.company_id::int                             AS id_compania,
    sm.origin_returned_move_id::int                AS origen_devolucion_move_id
  FROM public.stock_move_line sml
  JOIN public.stock_move sm              ON sm.id = sml.move_id
  LEFT JOIN public.stock_picking sp      ON sp.id = sm.picking_id
  LEFT JOIN picking_type ptp             ON ptp.id = sm.picking_type_id
  LEFT JOIN public.product_product pp    ON pp.id = COALESCE(sml.product_id, sm.product_id)
  LEFT JOIN public.product_template pt   ON pt.id = pp.product_tmpl_id
  LEFT JOIN pt_names ptn                 ON ptn.id = pt.id
  LEFT JOIN public.product_category pc   ON pc.id = pt.categ_id
  LEFT JOIN uom u                        ON u.id = sm.product_uom
  LEFT JOIN public.purchase_order_line pol ON pol.id = sm.purchase_line_id
  LEFT JOIN public.purchase_order po       ON po.id = pol.order_id
  LEFT JOIN public.mrp_production mp       ON mp.id = sm.production_id
),
/* ================= scrap = avería (egreso) */
base_scrap AS (
  SELECT
    ss.id::int                                   AS id_evento,
    COALESCE(ss.date_done,ss.create_date)::timestamp AS fecha,
    ss.product_id::int                           AS id_producto,
    COALESCE(pp.default_code,'SIN_CODIGO')::text AS codigo_producto,
    ptn.producto_nombre::text                    AS nombre_producto,
    pt.categ_id::int                             AS id_categoria,
    COALESCE(pc.name,'Sin categoría')::text      AS nombre_categoria,
    ss.location_id::int                          AS id_ubic_origen,
    ss.scrap_location_id::int                    AS id_ubic_destino,
    (ss.scrap_qty * -1)::numeric(18,6)           AS cantidad,
    u.uom_name::text                             AS unidad_medida,
    'scrap'::text                                AS picking_type_codigo,
    'scrap'::text                                AS tipo_evento,
    COALESCE(ss.state,'done')::text              AS estado_move,
    ss.picking_id::int                           AS id_picking,
    ''::text                                     AS codigo_picking,
    NULL::int                                    AS id_orden_compra,
    NULL::text                                   AS codigo_orden_compra,
    ss.production_id::int                        AS id_orden_produccion,
    ''::text                                     AS codigo_orden_produccion,
    ss.lot_id::int                               AS id_lote,
    ''::text                                     AS lote_serie,
    COALESCE(ss.origin,'')::text                 AS referencia,
    ss.company_id::int                           AS id_compania,
    ss.id::int                                   AS id_averia
  FROM public.stock_scrap ss
  LEFT JOIN public.product_product pp  ON pp.id = ss.product_id
  LEFT JOIN public.product_template pt ON pt.id = pp.product_tmpl_id
  LEFT JOIN pt_names ptn               ON ptn.id = pt.id
  LEFT JOIN public.product_category pc ON pc.id = pt.categ_id
  LEFT JOIN uom u                      ON u.id = ss.product_uom_id
),
/* ========= explotar transferencias en 2 filas (egreso e ingreso) */
mov_transfer_expand AS (
  -- egreso en origen
  SELECT bm.*, bm.id_ubic_origen AS id_almacen, (-ABS(bm.cantidad))::numeric(18,6) AS cantidad_neta, 'transferencia_egreso'::text AS subtipo, NULL::int AS id_averia
  FROM base_mov bm WHERE bm.tipo_evento = 'transferencia'
  UNION ALL
  -- ingreso en destino
  SELECT bm.*, bm.id_ubic_destino AS id_almacen, (ABS(bm.cantidad))::numeric(18,6) AS cantidad_neta, 'transferencia_ingreso'::text AS subtipo, NULL::int AS id_averia
  FROM base_mov bm WHERE bm.tipo_evento = 'transferencia'
),
/* ========= filas finales unificadas (recep/entrega + transfer expand + scrap + ajustes) */
f AS (
  -- recepciones y entregas (una fila)
  SELECT bm.*, 
         CASE WHEN bm.tipo_evento='recepcion' THEN bm.id_ubic_destino
              WHEN bm.tipo_evento='entrega'   THEN bm.id_ubic_origen
              ELSE bm.id_ubic_destino END::int AS id_almacen,
         bm.cantidad::numeric(18,6)            AS cantidad_neta,
         NULL::text                             AS subtipo,
         NULL::int                              AS id_averia
  FROM base_mov bm
  WHERE bm.tipo_evento IN ('recepcion','entrega')

  UNION ALL
  -- transferencias (dos filas)
  SELECT * FROM mov_transfer_expand

  UNION ALL
  -- scrap (avería)
  SELECT bs.*, bs.id_ubic_origen AS id_almacen, bs.cantidad::numeric(18,6) AS cantidad_neta, NULL::text AS subtipo, bs.id_averia
  FROM base_scrap bs

  UNION ALL
  -- ajustes de inventario (tratados aparte si los tienes; aquí como movimiento simple)
  SELECT bm.*,
         CASE WHEN bm.picking_type_codigo='inventory' AND bm.cantidad&gt;=0 THEN bm.id_ubic_destino
              WHEN bm.picking_type_codigo='inventory' AND bm.cantidad&lt;0  THEN bm.id_ubic_origen
              ELSE bm.id_ubic_destino END::int AS id_almacen,
         bm.cantidad::numeric(18,6) AS cantidad_neta,
         'ajuste'::text AS subtipo,
         NULL::int AS id_averia
  FROM base_mov bm
  WHERE bm.picking_type_codigo='inventory'
)
SELECT
  /* ==== Claves para lookups a Dim ==== */
  CAST(to_char(f.fecha,'YYYYMMDD') AS int)             AS FechaKey,
  f.id_producto                                        AS Id_Producto,      -- lookup a DimProducto
  f.id_almacen                                         AS Id_Almacen,       -- lookup a DimAlmacen
  COALESCE(f.id_orden_compra,0)                        AS Id_Orden_Compra,  -- lookup a DimOrdenCompra (usa 0=Desconocido)
  COALESCE(f.id_averia,0)                              AS Id_Averia,        -- lookup a DimAveria (0 si no aplica)

  /* ==== Métricas ==== */
  CASE
    WHEN f.tipo_evento='recepcion' THEN ABS(f.cantidad_neta)
    WHEN f.tipo_evento='transferencia' AND f.subtipo='transferencia_ingreso' THEN ABS(f.cantidad_neta)
    ELSE 0::numeric(18,6)
  END                                                  AS Cantidad_Ingresada,

  CASE
    WHEN f.tipo_evento='entrega' THEN ABS(f.cantidad_neta)
    WHEN f.tipo_evento='transferencia' AND f.subtipo='transferencia_egreso' THEN ABS(f.cantidad_neta)
    ELSE 0::numeric(18,6)
  END                                                  AS Cantidad_Descargada,

  CASE
    WHEN f.tipo_evento IN ('recepcion','entrega') AND f.origen_devolucion_move_id IS NOT NULL
      THEN ABS(f.cantidad_neta)
    ELSE 0::numeric(18,6)
  END                                                  AS Cantidad_Devoluciones,

  CASE WHEN f.tipo_evento='scrap' THEN ABS(f.cantidad_neta) ELSE 0::numeric(18,6) END AS Cantidad_Averiada,

  /* Inventario físico / diferencia: deja NULL si no tienes conteos */
  NULL::numeric(18,6)                                   AS Inventario_Fisico,
  NULL::numeric(18,6)                                   AS Diferencia_Inventario,

  /* Recepción aprobada/rechazada (por movimiento) */
  CASE WHEN f.tipo_evento='recepcion' AND f.estado_move='done'   THEN 1 ELSE 0 END AS Recepcion_Aprobada,
  CASE WHEN f.tipo_evento='recepcion' AND f.estado_move='cancel' THEN 1 ELSE 0 END AS Recepcion_Rechazada,

  /* Stock mínimo y Stock/Estado (snapshot) */
  /* Para mínimos: une a stock_warehouse_orderpoint si lo tienes por product/location y reemplaza el 0 */
  0::numeric(18,6)                                      AS Stock_Minimo,
  COALESCE(q.qty_on_hand - q.qty_reserved,0)::numeric(18,6) AS Stock,
  CASE
    WHEN COALESCE(q.qty_on_hand - q.qty_reserved,0) &lt;= 0 THEN 'Agotado'
    WHEN COALESCE(q.qty_on_hand - q.qty_reserved,0) &lt;= 0 /* &lt;--- reemplaza por Stock_Minimo si lo unes */ THEN 'Por agotarse'
    ELSE 'Disponible'
  END::text                                             AS Estado_Stock,

  /* Otros */
  f.tipo_evento::text                                   AS Tipo_Evento,
  f.lote_serie::text                                    AS Lote_Serie,
  NOW()                                                 AS CreatedAt

FROM f
LEFT JOIN quant q
  ON q.product_id = f.id_producto
 AND q.location_id = f.id_almacen;</property>
